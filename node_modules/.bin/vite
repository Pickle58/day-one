#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules/vite/bin/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules/vite/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules/vite/bin/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules/vite/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@7.3.1_@types+node@25.2_df4ccc65ffaef95ac759f81a85b4310d/node_modules/vite/bin/vite.js" "$@"
fi
