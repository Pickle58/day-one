#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules/sanity/bin/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules/sanity/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules/sanity/bin/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules/sanity/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules:/proc/cygdrive/c/Users/jpilk/Documents/Projects/sanity-course/day-one/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules/sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../.pnpm/sanity@5.9.0_@emotion+is-pr_160ab689f3bf77e617376811a985d500/node_modules/sanity/bin/sanity" "$@"
fi
